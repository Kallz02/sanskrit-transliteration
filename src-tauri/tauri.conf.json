{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "diacritics-for-sanskrit-transliteration",
  "mainBinaryName": "diacritics-for-sanskrit-transliteration",
  "version": "1.0.4",
  "identifier": "com.diacritics.dev",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Diacritics For Sanskrit transliterations v1.0.4",
        "width": 400,
        "height": 300,
        "visible": true,
        "useHttpsScheme": true
      }
    ]
  }
}